{"name":"Jeopardy","openedFile":"start.js","images":{},"files":{"index.html":{"scrollTop":0,"scrollLeft":0,"selectionStart":31,"selectionEnd":31,"lines":["<!doctype html>","<html>","  <head>","    <title>Jeopardy</title>","    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">","    <meta charset=\"UTF-8\">","    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">","    <style file=\"design.css\"></style>","    <style file=\"game.css\"></style>","    <script file=\"start.js\"></script>","    <script id=\"data\">var data = [['Q1', 'A1', 'Cat 1']];</script>","  </head>","  <body file=\"body.html\"></body>","</html>"]},"design.css":{"scrollTop":221,"scrollLeft":0,"selectionStart":313,"selectionEnd":313,"lines":["* {","  box-sizing: border-box;","  transition: all .6s;","}","body {","  background-color: #30395e;","}","h1 {","  text-align: center;","  padding: 0;","  margin: 5px;","}","table, tr, th, td {","  border: 1px solid black;","  border-collapse: collapse;","}","table {","  margin: auto;","  background-color: white;","}","td, th {","  padding: 5px;","}","tr {","  min-height: 18px;","}","td {","  text-align: left;","}","btn {","  cursor: pointer;","  margin: 5px;","  display: inline-block;","  padding: 4px;","  border-radius: 5px;","  background-color: #141414;","  color: white;","}","btn:hover {","  box-shadow: 0 0 3px gold;","}","#edit {","  position: fixed;","  text-align: center;","  max-height: calc(100% - 10px);","  top: 125%;","  left: 0;","  right: 0;","  width: fit-content;","  max-width: calc(100% - 200px);","  margin: auto;","  background-color: #303030;","}","#edit-btn {","  position: fixed;","  top: 5px;","  left: 5px;","  font-size: 80%;","}","#toolbox {","  position: fixed;","  top: 125%;","  font-size: 80%;","}","#tooltip {","  position: fixed;","  top: 125%;","  left: 0;","  right: 0;","  width: 200px;","  box-shadow: 0 0 4px black;","  padding: 4px;","  background-color: silver;","}","#categories {","  min-width: 250px;","  padding: 2px;","  border-radius: 5px;","  box-shadow: 0 0 3px black;","  margin 4px;","}"]},"start.js":{"scrollTop":0,"scrollLeft":0,"selectionStart":1582,"selectionEnd":1582,"lines":["addEventListener('error', e => {if (opener) opener.postMessage({lineno: e.lineno, colno: e.colno, message: e.message}, '*')})","addEventListener('load', load)","","var selectedRow","","addEventListener('keydown', e => {","  if (e.key === 'e' && e.ctrlKey) {","    e.preventDefault()","    showEdit()","  }","})","","function load() {","  let tippers = document.querySelectorAll('[tooltip]')","  tippers.forEach(tipper => {","    loadToolTipEvent(tipper)","  })","  render()","}","","function loadToolTipEvent(elem) {","  let tooltip = document.getElementById('tooltip')","  elem.onmousemove = function() {","    let {left, top, height} = elem.getBoundingClientRect()","    tooltip.innerHTML = elem.getAttribute('tooltip')","    tooltip.style.top = top + height + 5 + 'px'","    tooltip.style.left = left + 'px'","  }","  elem.onmouseleave = function() {","    tooltip.style.top = tooltip.style.left = ''","  }","}","","function changeCategories() {","  let cats = document.querySelectorAll('#edit table select')","  cats.forEach(updateSelection)","}","","function showEdit() {","  document.getElementById('edit').style.top = '5px'","  let table = document.querySelector('#edit table')","  while (table.rows.length > 1) table.rows[1].remove()","  data.forEach(d => addQuestion(d[0], d[1], d[2]))","}","","function hideEdit() {","  ;['edit', 'toolbox'].map(id => document.getElementById(id)).forEach(e => e.style.top = '')","}","","function addQuestion(question, answer, category, index) {","  let table = document.querySelector('#edit table')","  let row = table.insertRow(index)","  let toolbox = document.getElementById('toolbox')","  row.onmousemove = function() {","    selectedRow = row","    let {top, left, width} = row.getBoundingClientRect()","    toolbox.style.top = top + 'px'","    toolbox.style.left = left + width + 'px'","  }","  row.innerHTML=`<td contentEditable>${question || ''}</td><td contentEditable>${answer || ''}</td><td><select></select></td>`","  updateSelection(row.querySelector('select'))","  if (category) row.querySelector('select').value = category","}","","function updateSelection(select) {","  let value = select.value","  select.innerText = ''","  let categories = document.getElementById('categories').value.split(/, ?/g)","  categories.forEach(c => {","    let option = document.createElement('option')","    option.value = c","    option.innerText = c","    select.append(option)","  })","  if (categories.includes(value)) select.value = value","}","","function render() {","  let colors = ['#5d6a9c', '#759c5d', '#9c825d', '#9c5d8d', '#6b5d9c', '#5d8e9c', '#969c5d', '#9c655d']","  let game = document.getElementById('game')","  let categories = document.getElementById('categories').value.split(/, ?/g)","  categories.forEach((c, i) => {","    let col = document.createElement('column')","    col.innerHTML = `<h1>${c}</h1>`","    col.style.backgroundColor = colors[i]","    let quests = data.filter(v => v[2] === c)","    game.append(col)","    quests.forEach(q => {","      let div = document.createElement('div')","      div.innerHTML = q[0]","      div.onclick = () => {","        div.innerHTML = q[1]","        div.style.backgroundColor = 'rgba(255, 255, 255, .1)'","        div.style.boxShadow = '0 0 5px green'","      }","      col.append(div)","    })","  })","}","","function save() {","  document.getElementById('game').innerHTML = ''","  data = []","  let rows = document.querySelectorAll('#edit table tr')","  for (let x = 1; x < rows.length; x++) {","    let cells = rows[x].cells","    data.push([cells[0].innerHTML, cells[1].innerHTML, cells[2].querySelector('select').value])","  }","  document.getElementById('data').innerHTML = 'var data = ' + JSON.stringify(data)","  let a = document.createElement('a')","  let blob = new Blob(['<!DOCTYPE html>' + document.documentElement.outerHTML], {type: 'text/html'})","  a.href = URL.createObjectURL(blob)","  a.download = 'Jeopardy'","  a.click()","}"]},"body.html":{"scrollTop":0,"scrollLeft":0,"selectionStart":156,"selectionEnd":167,"lines":["<div id=\"game\"></div>","<btn id=\"edit-btn\" tooltip=\"Modify questions/answers.\" onclick=\"showEdit()\">edit</btn>","<div id=\"edit\">","  <input value=\"Cat 1, Cat 2\" id=\"categories\" placeholder=\"categories...\" tooltip=\"Seperate categories with a comma (,)\" onchange=\"changeCategories(); this.setAttribute('value', this.value)\">","  <table>","    <tr>","      <th>Question</th><th>Answer</th><th>Category</th>","    </tr>","  </table>","  <btn onclick=\"addQuestion()\">Add Question</btn>","  <div>","    <btn onclick=\"hideEdit(); save(); render()\">Save Changes</btn>","    <btn onclick=\"hideEdit()\">Cancel</btn>","  </div>","</div>","","<div id=\"toolbox\">","  <btn onclick=\"addQuestion('', '', '', selectedRow.rowIndex)\">Add Row</btn>","  <btn onclick=\"selectedRow.remove(); this.parentElement.style.top = ''\">Delete Row</btn>","</div>","<div id=\"tooltip\"></div>",""]},"game.css":{"lines":["column {","  display: flex;","  flex-direction: column;","  box-shadow: 0 0 3px white;","  padding: 10px;","  flex-grow: 1;","  flex-basis: 0;","  margin: 25px;","  text-align: center;","}","","column > div {","  padding: 5px;","  background-color: rgba(0, 0, 0, .1);","  flex-grow: 1;","  flex-basis: 0;","  display: flex;","  align-items: center;","  justify-content: center;","  font-size: 125%;","  border-radius: 5px;","  box-shadow: 0 0 3px white;","  cursor: pointer;","}","","column > div:hover {","  box-shadow: 0 0 3px gold;","}","","#game {","  position: fixed;","  display: flex;","  justify-content: space-around;","  top: 0;","  left: 0;","  right: 0;","  bottom: 0;","}"],"scrollLeft":0,"scrollTop":0,"selectionStart":223,"selectionEnd":239}}}